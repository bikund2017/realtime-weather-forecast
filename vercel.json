{
  "version": 2,
  "rewrites": [
    {
      "source": "/style.css",
      "destination": "/frontend/style.css"
    },
    {
      "source": "/main.js",
      "destination": "/frontend/main.js"
    },
    {
      "source": "/sw.js",
      "destination": "/frontend/sw.js"
    },
    {
      "source": "/(.*)\\.(css|js|html|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)",
      "destination": "/frontend/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}
